{
    "manifest_version": 2,
  
    "name": "Trophy Collector Plugin",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
      },  
    "browser_action": {
     "default_icon": "icon.png"
    },
     "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.5.1.min.js","popup.js"]
        }
      ]
  }